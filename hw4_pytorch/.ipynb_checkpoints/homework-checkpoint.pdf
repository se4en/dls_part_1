    "        blocks['relu' + str(i+1)] = nn.ReLU()\n",
    "        blocks['dropout' + str(i+1)] = nn.Dropout(p=0.5)\n",
    "    blocks['end_linear'] = nn.Linear(100, 3)\n",
    "    return nn.Sequential(blocks)\n",
